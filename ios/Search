import SwiftUI

struct Search: View {
    var body: some View {
        Button(action:{}) {
            
            HStack {
                Image(systemName: "magnifyingglass")
                
                Text("Search doctor or health issue")
                    .font(.custom("Poppins", size: 15))
                    .foregroundColor(Color.gray)
            }
            .padding()
            Spacer()
        }
        .frame()
        .background(Color.lightGray)
        .clipShape(RoundedRectangle(cornerRadius: 20))
        .padding()
    }
}


